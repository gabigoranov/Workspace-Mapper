<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Overrides for unpleasing styles forced by FluentTheme -->
    <StaticResource x:Key="ButtonBackground" ResourceKey="Color.Card" />
    <StaticResource x:Key="ButtonBackgroundPointerOver" ResourceKey="Color.Background" />
    <StaticResource x:Key="ButtonForegroundPointerOver" ResourceKey="Color.Text.Primary" />
    <StaticResource x:Key="ButtonBackgroundPressed" ResourceKey="Color.Background.Subtle" />
    <StaticResource x:Key="ButtonForegroundPressed" ResourceKey="Color.Text.Primary" />
    
    <StaticResource x:Key="TextControlBackgroundFocused" ResourceKey="Color.Background" />
    <StaticResource x:Key="TextControlForegroundFocused" ResourceKey="Color.Text.Primary" />
    <StaticResource x:Key="TextControlPlaceholderForegroundFocused" ResourceKey="Color.Text.Primary" />

    <StaticResource x:Key="TextControlBackground" ResourceKey="Color.Background.Subtle" />
    <StaticResource x:Key="TextControlBackgroundPointerOver" ResourceKey="Color.Background.Subtle" />
    <StaticResource x:Key="TextControlForegroundPointerOver" ResourceKey="Color.Text.Primary" />
    <StaticResource x:Key="TextControlPlaceholderForegroundPointerOver" ResourceKey="Color.Text.Primary" />

    <StaticResource x:Key="TextControlBackgroundPressed" ResourceKey="Color.Background" />
    
    <StaticResource x:Key="TextBlockForeground" ResourceKey="Color.Text.Primary" />
    
    <StaticResource x:Key="FlyoutPresenterBackground" ResourceKey="Color.Card" />
    <StaticResource x:Key="MenuFlyoutPresenterBackground" ResourceKey="Color.Card" />
    <StaticResource x:Key="FlyoutPresenterBorderBrush" ResourceKey="Color.Border" />
    <StaticResource x:Key="MenuFlyoutPresenterBorderBrush" ResourceKey="Color.Border" />

</ResourceDictionary>